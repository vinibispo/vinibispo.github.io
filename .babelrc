{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver", {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@services": "./src/services",
          "@utils": "./src/utils",
          "@pages": "./src/pages",
          "@hooks": "./src/hooks",
          "@hoc": "./src/hoc",
          "@contexts": "./src/contexts"
        }
      }
    ]
  ]
}
